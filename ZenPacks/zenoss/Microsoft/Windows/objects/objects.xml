<object id='/zport/dmd/Events/Status/instances/WindowsServiceLog' module='Products.ZenEvents.EventClassInst' class='EventClassInst'>
<property type="text" id="transform" mode="w" >
import re

@transact
def change_properties(currentstate):
    if getattr(component, 'state') != currentstate:
            setattr(component, 'state', currentstate)

msg = evt.summary
summaryObj = re.match(r'Service (.*): (.*) has changed to (.*) state(.*)', msg)

change_properties(summaryObj.group(3))
</property>

<!-- ('', 'zport', 'dmd', 'Events', 'Status', 'instances', 'IISSiteStatus') -->
<object id='/zport/dmd/Events/Status/instances/IISSiteStatus' module='Products.ZenEvents.EventClassInst' class='EventClassInst'>
<property type="text" id="transform" mode="w" >
import re

@transact
def change_properties(currentstate):
    if getattr(component, 'status') != currentstate:
        setattr(component, 'status', currentstate)

msg = evt.summary
summaryObj = re.match(r'IIS Service (.*) is in (.*) state(.*)', msg)

change_properties(summaryObj.group(2))
</property>
